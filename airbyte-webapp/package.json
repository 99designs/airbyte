{
  "name": "airbyte-webapp",
  "version": "0.29.15-alpha",
  "private": true,
  "scripts": {
    "dist": "webpack -c .webpack/webpack.config",
    "lint": "eslint .",
    "prettier:check": "prettier --check --ignore-unknown .",
    "prettier:format": "prettier --write --ignore-unknown .",
    "start": "webpack serve -c .webpack/webpack.config",
    "test": "jest",
    "typecheck": "tsc --project ./tsconfig.json",
    "storybook": "start-storybook -p 9009 -s public --quiet"
  },
  "dependencies": {
    "@asayerio/tracker": "^5.6.8",
    "@fortawesome/fontawesome-svg-core": "^1.2.27",
    "@fortawesome/free-brands-svg-icons": "^5.15.2",
